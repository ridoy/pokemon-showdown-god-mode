{
  "name": "Pokemon Showdown God Mode",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Pokemon Showdown God Mode",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://play.pokemonshowdown.com/*"
      ],
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/jquery-2.1.4.min.js",
        "src/inject/calc/calc.js",
        "src/inject/data/gen7formatsdata.min.js",
        "src/inject/damage-calculator.js",
        "src/inject/main.js"
      ]
    }
  ]
}
