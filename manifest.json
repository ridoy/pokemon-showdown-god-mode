{
  "name": "Pokemon Showdown God Mode",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Pokemon Showdown God Mode",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "default_locale": "en",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://play.pokemonshowdown.com/*"
      ],
      "js": [
        "js/data/production.min.js",
        "js/jquery-2.1.4.min.js",
        "src/inject/calc.js",
        "src/inject/main.js"
      ]
    }
  ]
}
